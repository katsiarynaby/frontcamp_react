{
  "name": "front_camp_2019",
  "version": "0.0.1",
  "description": "Front Camp 2019 by Katsiaryna Kananchuk",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --output-public-path=/dist/  --mode development --progress --open --hot",
    "build": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:update": "npm test -- -u",
    "test:reporter": "jest --reporters",
    "debug": "set NODE_ENV=debug npm test"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "scss"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/src/setupTest.js"
    ],
    "reporters": [
      "default",
      ["./node_modules/jest-html-reporter", {
        "pageTitle": "Test Report",
        "includeFailureMsg": true
    }]
    ],
    "preset": "jest-puppeteer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katsiarynaby/frontcamp_react.git"
  },
  "author": "Katsiaryna Kananchuk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katsiarynaby/frontcamp_react/issues"
  },
  "homepage": "https://github.com/katsiarynaby/frontcamp_react#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.7.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.1",
    "enzyme-to-json": "^3.4.4",
    "identity-obj-proxy": "^3.0.0",
    "jest-cli": "^25.1.0",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "puppeteer": "^2.1.1",
    "puppeteer-core": "^1.9.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "ttf-loader": "^1.0.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "redux-logger": "^3.0.6",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-stage-0": "^7.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "babel-jest": "^25.1.0",
    "classnames": "^2.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "jest-html-reporter": "^2.8.0",
    "jest-puppeteer": "^4.4.0",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.3",
    "react-router": "^3.2.5",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.12.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  }
}
